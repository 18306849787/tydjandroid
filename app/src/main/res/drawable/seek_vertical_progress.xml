<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="8dp" android:gravity="center">
        <shape android:shape="rectangle">
            <solid android:color="#ffe9e9e9" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="8dp" android:gravity="center">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="#fffb7468" />
                <corners android:radius="4dp" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:gravity="center" android:height="8dp">
        <clip>
            <shape android:shape="rectangle">
                <solid android:color="#fffb7468" />
                <corners android:radius="4dp" />
            </shape>
        </clip>
    </item>
</layer-list>
